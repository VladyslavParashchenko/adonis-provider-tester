{
	"name": "adonis-provider-tester",
	"version": "1.0.0",
	"description": "Test runner for adonis providers",
	"scripts": {
		"clean": "del build",
		"lint": "eslint . --ext=.ts --fix",
		"compile": "npm run lint && npm run clean && tsc && npm run copy:files",
		"build": "npm run compile",
		"prepare": "npm run build",
		"copy:files": "cp 'src/adonis-app/source/.adonisrc.json' 'src/adonis-app/source/.env' 'build/src/adonis-app/source/'"
	},
	"author": "Vladyslav Parashchenko",
	"license": "MIT",
	"dependencies": {
		"@adonisjs/ace": "^11.0.1",
		"@adonisjs/core": "^5.1.7",
		"@adonisjs/fold": "^8.1.2",
		"@adonisjs/logger": "^4.0.3",
		"@adonisjs/ioc-transformer": "^2.2.1",
		"@adonisjs/mrm-preset": "^4.0.1",
		"@poppinss/dev-utils": "^1.1.3",
		"@types/node": "^14.17.1",
		"chai": "^4.3.4",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.2.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-adonis": "^1.3.1",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^4.3.8",
		"mrm": "^2.3.3",
		"np": "^6.3.2",
		"npm-audit-html": "^1.4.1",
		"prettier": "^2.0.5",
		"reflect-metadata": "^0.1.13",
		"source-map-support": "^0.5.19",
		"ts-node": "^8.10.2",
		"typescript": "^4.3.2",
		"pino-pretty": "^4.7.1",
		"del-cli": "^3.0.1"
	},
	"main": "build/index.js"
}
